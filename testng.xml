<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BookMyShow Test Suite" verbose="1" parallel="none">

	<listeners>
		<listener class-name="reports.ExtentTestNGListener"></listener>
	</listeners>

	<test name="BookMyShow Tests on chrome">
  	<parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="tests.LoginTest"/>
      <class name="tests.CityTest"/>
      <class name="tests.MovieTest"/>
      <class name="tests.GiftCardTest"/>
    </classes>
  </test>


</suite>




